{
  "expo": {
    "name": "E-DentCare+",
    "slug": "dentist-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tooth.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ron19102004.dentistapp",
      "supportsTablet": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [{ "scheme": "fb", "host": "profile" }],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/tooth.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/tooth.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e6ff2e5a-a1a2-4812-98f2-27136dbd9817"
      }
    }
  }
}
